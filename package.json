{
  "name": "sonub",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "hmr": "ng serve --hmr -e=hmr",
    "hmr:ssl:dev": "ng serve --hmr -e=dev --ssl true --ssl-key './tmp/ssl/privkey.pem' --ssl-cert './tmp/ssl/fullchain.pem' --host sonub.com --port 443",
    "hmr:ssl:prod": "ng serve --hmr -e=prod --ssl true --ssl-key './tmp/ssl/privkey.pem' --ssl-cert './tmp/ssl/fullchain.pem' --host sonub.com --port 443",
    "s": "sudo npm run ssl",
    "publish": "npm run build:prod:aot:www:web && npm run scp-update-patch",
    "republish": "npm run build:prod:aot:www:web && npm run scp",
    "scp": "cd www && scp -r * sonub@www.sonub.com:./www",
    "scp-update-patch": "cd www && scp index.html *.css *.js sonub@www.sonub.com:./www",
    "seo": "ng build --watch --output-path=/Users/thruthesky/www/wordpress --delete-output-path=false",
    "seo:prod": "ng build --prod --watch --output-path=/Users/thruthesky/www/wordpress --delete-output-path=false",
    "ssl": "ng serve --ssl true --ssl-key './tmp/ssl/privkey.pem' --ssl-cert './tmp/ssl/fullchain.pem' --host sonub.com --port 443",
    "ssl:prod": "ng serve -e=prod --ssl true --ssl-key './tmp/ssl/privkey.pem' --ssl-cert './tmp/ssl/fullchain.pem' --host sonub.com --port 443",
    "sssl": "ng serve --ssl true --ssl-key ./tmp/ssl/privkey.pem --ssl-cert ./tmp/ssl/fullchain.pem --host sonub.com --port 443",
    "setup": "npm run build-ie-polyfills",
    "build-ie-polyfills": "node node_modules/webpack/bin/webpack.js -p src/ie-polyfills.js src/assets/js/ie-polyfills.min.js",
    "build:www": "ng build --output-path=www",
    "build:prod:www": "ng build --prod --output-path=www",
    "build:prod:aot:www:web": "ng build --prod --aot --output-path=www --base-href=/",
    "build:android": "ng build --output-path=www --base-href=/android_asset/www/"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.0.0",
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/forms": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@angular/router": "^4.0.0",
    "@angular/service-worker": "^1.0.0-beta.16",
    "@ng-bootstrap/ng-bootstrap": "^1.0.0-alpha.29",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.4.1",
    "firebase": "^4.1.3",
    "rxjs": "^5.1.0",
    "web-animations-js": "^2.3.1",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@angular/cli": "1.2.0",
    "@angular/compiler-cli": "^4.0.0",
    "@angular/language-service": "^4.0.0",
    "@angular/platform-server": "^4.3.3",
    "@angularclass/hmr": "^2.1.3",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "~3.0.1",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-pwa-tools": "0.0.15",
    "protractor": "~5.1.2",
    "ts-node": "~3.0.4",
    "tslint": "~5.3.2",
    "typescript": "~2.3.3"
  }
}
